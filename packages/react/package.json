{
  "name": "@weektime-picker/react",
  "version": "0.2.5",
  "description": "Week time grid component for React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite examples --config ./vite.config.ts",
    "build": "vite build && npx tsc -p tsconfig.build.json --emitDeclarationOnly",
    "clean": "rimraf dist",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShamProfessor/week-time-picker.git"
  },
  "keywords": [
    "time-picker",
    "week-time-grid",
    "react"
  ],
  "author": "ShamProfessor",
  "license": "MIT",
  "homepage": "https://github.com/ShamProfessor/week-time-picker#readme",
  "bugs": {
    "url": "https://github.com/ShamProfessor/week-time-picker/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@weektime-picker/core": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "jsdom": "^26.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
