{
  "name": "@weektime-picker/vue",
  "version": "0.2.5",
  "description": "Week time grid component for Vue 3",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite examples --config ./vite.config.ts",
    "build": "vite build && npx vue-tsc -p tsconfig.build.json --emitDeclarationOnly",
    "clean": "rimraf dist",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShamProfessor/week-time-picker.git"
  },
  "keywords": [
    "time-picker",
    "week-time-grid",
    "vue"
  ],
  "author": "ShamProfessor",
  "license": "MIT",
  "homepage": "https://github.com/ShamProfessor/week-time-picker#readme",
  "bugs": {
    "url": "https://github.com/ShamProfessor/week-time-picker/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@weektime-picker/core": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/test-utils": "^2.3.2",
    "jsdom": "^26.1.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.30.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.6.4"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
