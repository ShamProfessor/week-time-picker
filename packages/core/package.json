{
  "name": "@weektime-picker/core",
  "version": "0.2.5",
  "description": "Core logic for weektime-picker component",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c && node scripts/postbuild.js",
    "clean": "rimraf dist",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShamProfessor/week-time-picker.git"
  },
  "keywords": [
    "time-picker",
    "week-time-grid"
  ],
  "author": "ShamProfessor",
  "license": "MIT",
  "homepage": "https://github.com/ShamProfessor/week-time-picker#readme",
  "bugs": {
    "url": "https://github.com/ShamProfessor/week-time-picker/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "development": {
        "import": "./src/index.ts",
        "require": "./dist/index.js",
        "types": "./src/index.ts"
      },
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.5",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
